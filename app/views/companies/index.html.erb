<paper-checkbox checked></paper-checkbox>

<table class="table table-condensed" ng-controller="CompanyCtrl">
  <input ng-model="name" />

  <tr ng-repeat="company in companies | filter:name">
    <td>{{company[0]}}</td>
    <td>{{company[1]}}</td>
    <td><a ng-href="companies/{{company[0]}}">Link</a></td>
  </tr>
</table>